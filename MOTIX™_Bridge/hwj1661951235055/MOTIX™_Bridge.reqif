            <SPEC-OBJECT LAST-CHANGE="2023-02-17T10:05:37" IDENTIFIER="BDBA-FFAI-EAEG-CAAB">
               <VALUES>
                  <ATTRIBUTE-VALUE-STRING THE-VALUE="BDBA-FFAI-EAEG-CAAB">
                     <DEFINITION>
                        <ATTRIBUTE-DEFINITION-STRING-REF>_stype_requirement_requirementID</ATTRIBUTE-DEFINITION-STRING-REF>
