            <SPEC-OBJECT LAST-CHANGE="2022-05-27T00:00:01" IDENTIFIER="FBJA-EDJE-GEIF-AJHJ">
               <VALUES>
                  <ATTRIBUTE-VALUE-STRING THE-VALUE="FBJA-EDJE-GEIF-AJHJ">
                     <DEFINITION>
